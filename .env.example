# BasketballBund Crawler Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Configuration (Client Side)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Service Key (Admin Operations)
# Used for writing minutes/stats. Keep this secure!
# NOTE: This key will be bundled in the client code but protected by the admin password in the UI.
VITE_SUPABASE_SERVICE_KEY=your_service_role_key_here

# Admin Access
# Password required to access the minutes management UI
VITE_ADMIN_PASSWORD=your_admin_password_here

# BasketballBund League Configuration (for crawler scripts)
LEAGUE_ID=49400

# Optional: Add any additional environment variables needed
# DEBUG=true
# LOG_LEVEL=INFO
